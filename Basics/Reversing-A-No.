// This program reverses a number that is entered by the user.

import java.util.Scanner;

public class ReversingANo {

	public static void main(String[] args) {
		
	
		System.out.println("Enter the no.");
		Scanner scn = new Scanner(System.in);
		int n = scn.nextInt();
		int sum = 0;
		
		while(n != 0) {
			int no = n%10;
			sum= sum*10+no;
			
			n = n/10;
			
		}
		System.out.println(sum);
	}

}
